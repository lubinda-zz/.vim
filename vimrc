runtime bundle/pathogen/autoload/pathogen.vim
execute pathogen#infect()

set nocompatible

syntax enable
filetype plugin on

colorscheme gruvbox
set background=dark

set number

set tabstop=2
set shiftwidth=2
set softtabstop=2
set expandtab

set autoindent
set smartindent

set incsearch
set hlsearch

set path+=**

set wildmenu
